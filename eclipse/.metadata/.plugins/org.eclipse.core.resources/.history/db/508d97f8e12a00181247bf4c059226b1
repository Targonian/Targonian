package targon.Sheep.Client.Modules;

import net.minecraft.util.Timer;
import net.minecraft.util.math.BlockPos;
import targon.Sheep.Client.Module;

public class Spammer extends Module {

	public Spammer() {
		super("Spammer", 24, "O");
	}

	public void onEnable() {
		setIS_TICKING(true);
		getMc().player
		.sendChatMessage("JOIN TARGONSKINGDOM.MCPRO.IO TODAY FOR AWESOME PRIZES AND MORE");
getMc().player.sendChatMessage("PVP,KITS,BUKKIT,PARTY AND MANY MORE");
		super.onEnable();
	}

	public void onDisable() {
		setIS_TICKING(false);
		super.onDisable();
	}

	long lastTime = System.currentTimeMillis();
	long nowTime = System.currentTimeMillis();

	public void onTick() {


		super.onTick();
	}
}
